<?xml version="1.0" encoding="UTF-8"?>
<bundle>
   <UIUserPlatformToolBarCreatePortlet>
      <label>
         <topic>Topic</topic>
         <poll>Poll</poll>
         <eventtask>Event/Task</eventtask>
         <upload>Upload a File</upload>
         <wiki>Wiki Page</wiki>

      </label>
   </UIUserPlatformToolBarCreatePortlet>
   <UICreateList>
      <label>
         <topic>topic</topic>
         <poll>poll</poll>
         <CreateANew>Create a new {1}:</CreateANew>
         <NoForum>Sorry, no forum is available yet to create a {1}. Start by creating your own space.</NoForum>
         <RequireSelectForum>Please select a forum.</RequireSelectForum>
      </label>
      <action>
         <Next>#{word.next}</Next>
         <Cancel>#{word.cancel}</Cancel>
      </action>
   </UICreateList>
   <UICreateTopic>
      <label>
         <SelectAForum>Select a Forum</SelectAForum>
         <location>In Location</location>
         <forumId>And Forum</forumId>
         <FilterForums>Filter Forums</FilterForums>
      </label>
   </UICreateTopic>
   <UICreatePoll>
      <label>
         <SelectAForum>Select Forum</SelectAForum>
         <location>In Location</location>
         <forumId>And Forum</forumId>
         <FilterForums>Filter Forums</FilterForums>
      </label>
   </UICreatePoll>
   <UICreateWiki>
      <label>
         <title>Create a new Wiki Page:</title>
         <location>In Location</location>
      </label>
   </UICreateWiki>
   <UICreateEvent>
      <label>
         <Title>#{word.title}</Title>
         <Choix>Add New:</Choix>
         <EndEvent>To</EndEvent>
         <StartEvent>From</StartEvent>
         <Calendar>Select Calendar</Calendar>
         <all-day>All Day</all-day>
         <defaultCalendarId>Default</defaultCalendarId>
      </label>
      <msg>
         <summary-field-required>The field "Title" is required.</summary-field-required>
         <fromDate-format>The "From" date format is incorrect.</fromDate-format>
         <toDate-format>The "To" date format is incorrect.</toDate-format>
         <logic-required>Sorry, you cannot create an event that ends before it starts.</logic-required>
         <cant-add-event-on-remote-calendar>You cannot add event to remote calendar</cant-add-event-on-remote-calendar>
         <add-successfully>
            <Event>The Event was added to {1}</Event>
            <Task>The Task was added to {1}</Task>
         </add-successfully>
         <add-unsuccessfully>{0} was not added.</add-unsuccessfully>
      </msg>
      <action>
         <Next>#{word.save}</Next>
         <Cancel>#{word.cancel}</Cancel>
      </action>
   </UICreateEvent>
    <UIUploadFile>
        <Select>Select File</Select>
        <action>
          <close>#{word.close}</close>
        </action>
    </UIUploadFile>
   <Choix>
      <label>
         <Event>Event</Event>
         <Task>Task</Task>
      </label>
   </Choix>
</bundle>
